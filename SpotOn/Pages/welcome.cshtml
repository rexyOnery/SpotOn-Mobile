@page
@model MeetSpotOn.Pages.welcomeModel
@{
    Layout = null;
}
 
<!DOCTYPE HTML>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover" />
    <title>Spot-On Mobile</title>
    <link rel="stylesheet" type="text/css" href="/styles/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/styles/style.css">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/fonts/css/fontawesome-all.min.css">
    <link rel="manifest" href="/_manifest.json" data-pwa-version="set_in_manifest_and_pwa_js">
    <link rel="apple-touch-icon" sizes="180x180" href="/app/icons/icon-192x192.png">
</head>

<body class="theme-dark">

    <div id="preloader"><div class="spinner-border color-highlight" role="status"></div></div>

    <div id="page">

        <div class="header header-fixed header-logo-left header-auto-show">
            <a href="/" class="header-title">Spot-On</a>
            <a href="#" data-menu="menu-main" class="header-icon header-icon-1"><i class="fas fa-bars"></i></a>
            <a href="#" data-toggle-theme class="header-icon header-icon-3 show-on-theme-dark"><i class="fas fa-sun"></i></a>
            <a href="#" data-toggle-theme class="header-icon header-icon-3 show-on-theme-light"><i class="fas fa-moon"></i></a>
            <a href="/login" class="header-icon header-icon-2"><i class="fas fa-lock"></i></a>
        </div>

        <div id="footer-bar" class="footer-bar-6">
            <a href="/construction"><i class="fa fa-box"></i><span>Artisan</span></a>
            <a href="/construction"><i class="fa fa-utensils"></i><span>Chef</span></a>
            <a href="/health" class="circle-nav active-nav"><i class="fa fa-stethoscope"></i><span>Health</span></a>
            <a href="/construction"><i class="fa fa-shopping-cart"></i><span>Products</span></a>
            <a href="/construction"><i class="fa fa-chalkboard-teacher"></i><span>Tutors</span></a>
        </div>

        <div class="page-title page-title-fixed">
            <h1>Spot-On</h1>
            <a href="/login" class="page-title-icon shadow-xl bg-theme color-theme"><i class="fa fa-lock-open"></i></a>
            <a href="#" class="page-title-icon shadow-xl bg-theme color-theme show-on-theme-light" data-toggle-theme><i class="fa fa-moon"></i></a>
            <a href="#" class="page-title-icon shadow-xl bg-theme color-theme show-on-theme-dark" data-toggle-theme><i class="fa fa-lightbulb color-yellow-dark"></i></a>
            <a href="#" class="page-title-icon shadow-xl bg-theme color-theme" data-menu="menu-main"><i class="fa fa-bars"></i></a>
        </div>
        <div class="page-title-clear"></div>


        <div class="page-content">

            <div data-card-height="300" class="card card-style shadow-xxl bg-1">
                <div class="card-bottom text-right">
                    <h6 class="color-white mt-3 mr-3 font-12 font-400">Powered by</h6>
                    <h6 class="color-white mb-3 mr-3 font-16 mt-n1">Start-Light</h6>
                </div>
                <div class="card-top  pt-3 pl-3">
                    <h1 class="color-white font-18 mb-0 opacity-50">A new Generation</h1>
                    <h1 class="color-white font-30 mt-n2 mb-5">Of Mobile Interaction</h1>
                </div>
                <div class="card-bottom text-left pl-3">
                    <p class="color-white mb-3 opacity-70 line-height-s">
                        @*Bootstrap 4 Based<br>
                        Mobile Kit & PWA*@
                    </p>
                </div>
                <div class="card-overlay bg-gradient opacity-60"></div>
            </div>

            <h4 class="ml-3 mb-4">Spot-On Packages!</h4>
            <div class="double-slider owl-carousel owl-no-dots">
                <div class="card card-style mx-0">
                    <div class="d-flex pt-3 pb-3">
                        <div class="align-self-center">
                            <i class="fa fa-heartbeat color-green-light ml-3 font-34 mt-1"></i>
                        </div>
                        <div class="align-self-center">
                            <h5 class="pl-2 ml-1 mb-0">Health <br> Care</h5>
                        </div>
                    </div>
                    <p class="px-3">
                        Provide you with on-the-spot health-care.
                    </p>
                </div>
                <div class="card card-style mx-0">
                    <div class="d-flex pt-3 pb-3">
                        <div class="align-self-center">
                            <i class="fa fa-box color-blue-light ml-3 font-34 mt-1"></i>
                        </div>
                        <div class="align-self-center">
                            <h5 class="pl-2 ml-1 mb-0">Artisan <br> Reach</h5>
                        </div>
                    </div>
                    <p class="px-3">
                        Access top quality artisans on the go.
                    </p>
                </div>
                <div class="card card-style mx-0">
                    <div class="d-flex pt-3 pb-3">
                        <div class="align-self-center">
                            <i class="fa fa-utensil-spoon color-gray-dark ml-3 font-34 mt-1"></i>
                        </div>
                        <div class="align-self-center">
                            <h5 class="pl-2 ml-1 mb-0">Chef <br> Ready</h5>
                        </div>
                    </div>
                    <p class="px-3">
                        Gorgeous meals for your taste's delight.
                    </p>
                </div>
                <div class="card card-style mx-0">
                    <div class="d-flex pt-3 pb-3">
                        <div class="align-self-center">
                            <i class="fa fa-cart-arrow-down color-brown-dark ml-3 font-34 mt-1"></i>
                        </div>
                        <div class="align-self-center">
                            <h5 class="pl-2 ml-1 mb-0">Products <br> Line</h5>
                        </div>
                    </div>
                    <p class="px-3">
                        Price bargain system on products.
                    </p>
                </div>
                <div class="card card-style mx-0">
                    <div class="d-flex pt-3 pb-3">
                        <div class="align-self-center">
                            <i class="fa fa-chalkboard color-yellow-dark ml-3 font-34 mt-1"></i>
                        </div>
                        <div class="align-self-center">
                            <h5 class="pl-2 ml-1 mb-0">Quality <br> Tutors</h5>
                        </div>
                    </div>
                    <p class="px-3">
                        Access mentors as tutors for that child.
                    </p>
                </div>
                <div class="card card-style mx-0">
                    <div class="d-flex pt-3 pb-3">
                        <div class="align-self-center">
                            <i class="fa fa-users-cog color-red-dark ml-3 font-34 mt-1"></i>
                        </div>
                        <div class="align-self-center">
                            <h5 class="pl-2 ml-1 mb-0">Best <br> Values</h5>
                        </div>
                    </div>
                    <p class="px-3">
                        More features brewing in our refinery.
                    </p>
                </div>
            </div>

            <div class="card card-style card-full-left">
                <div class="card card-style card-full-left m-0 p-4 bg-10" data-card-height="350">
                    <div class="card-center text-center">
                        <h6 class="color-white mb-0 opacity-30">MEET Spot-On</h6>
                        <h1 class="color-white font-800 font-30 line-height-xl">For on-the-spot  <br>Quality Engagement</h1>
                        <p class="color-white font-16 opacity-60 line-height-l boxed-text-l mb-0">
                            We create meeting hubs for spontenuous interactions with effective reach and stunning
                            verifyable results with instant feedbacks
                        </p>
                    </div>
                    <div class="card-overlay bg-black opacity-80"></div>
                </div>
                <div class="content">
                    <p class="mb-n1 font-600 color-highlight">Because you Matter!</p>
                    <h1>Elite Quality</h1>
                    <p>
                        We know you're getting the absolute best with our packs. We'll be here listening for your
                        feedbacks as we go on this journey together!
                    </p>
                </div>
            </div>

            <div class="card card-style">
                <div class="content mb-0">
                    <p class="font-600 mb-n1 color-highlight">Spot-On Comes Packed with</p>
                    <h1>Latest Trends</h1>
                    <div class="list-group list-custom-large">
                        <a href="#">
                            <i class="fa font-14 fa-info rounded-sm shadow-m bg-red-dark color-white"></i>
                            <span>Instant Communication</span>
                            <strong>Gain Accelerated Information</strong>
                            <i class="fa fa-angle-right"></i>
                        </a>
                        <a href="#">
                            <i class="fa font-14 fa-mobile-alt rounded-sm shadow-m bg-green-dark color-white"></i>
                            <span>Minimal Contact</span>
                            <strong>The New Normal</strong>
                            <i class="fa fa-angle-right"></i>
                        </a>
                        <a href="#" data-toggle-theme-switch>
                            <i class="fa font-14 fa-moon rounded-sm shadow-m bg-blue-dark color-white"></i>
                            <span>Dark Mode</span>
                            <strong>Automatic and Manual</strong>
                            <div class="custom-control ios-switch switch-s mr-n1 mt-3 pt-1">
                                <input type="checkbox" class="ios-input" id="toggle-dark-switch">
                                <label class="custom-control-label" for="toggle-dark-switch"></label>
                            </div>
                            <i class="fa fa-angle-right"></i>
                        </a>
                        <a href="#">
                            <i class="fa font-14 fa-book-open rounded-sm shadow-m bg-orange-dark color-white"></i>
                            <span>Knowledge Based</span>
                            <strong>Enlightenment For Your Kids</strong>
                            <i class="fa fa-angle-right"></i>
                        </a>
                        <a class="border-0" href="#">
                            <i class="fa font-14 fa-tint rounded-sm shadow-m bg-yellow-dark color-white"></i>
                            <span>Taste Appeal</span>
                            <strong>It's Just Tap & Order</strong>
                            <i class="fa fa-angle-right"></i>
                        </a>
                    </div>
                </div>
            </div>



            <div class="card card-style p-4 bg-31" data-card-height="350">
                <div class="card-center text-center">
                    @*<h6 class="mb-0 color-highlight">Be Spot-On Today</h6>*@
                    <h1 class="font-800 color-white font-30 line-height-xl">Be Spot-On <br>Today</h1>
                    <p class="font-16 color-white opacity-50 line-height-l boxed-text-l">
                        Spot-On is packed to accommodate your interests in health-care, artisans, chefs, products, and tutors with many more features to be added in the near future.
                    </p>
                    <a href="/aboutregistration" class="btn btn-center-m btn-m gradient-blue rounded-s font-700 text-uppercase">Get Spot-On</a>
                </div>
                <div class="card-overlay bg-black opacity-80"></div>
            </div>


            <div data-menu-load="menu-footer.html"></div>
        </div>
        <!-- Page content ends here-->
        <!-- Main Menu-->
        <div id="menu-main" class="menu menu-box-left rounded-0" data-menu-load="menu-main.html" data-menu-width="280" data-menu-active="nav-homepages"></div>

        <!-- Share Menu-->
        <div id="menu-share" class="menu menu-box-bottom rounded-m" data-menu-load="menu-share.html" data-menu-height="370"></div>

        <!-- Colors Menu-->
        <div id="menu-colors" class="menu menu-box-bottom rounded-m" data-menu-load="menu-colors.html" data-menu-height="480"></div>

       
    </div>
    <script type="text/javascript" src="/scripts/jquery.js"></script>
    <script type="text/javascript" src="/scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="/scripts/custom.js"></script>
    <script type="text/javascript">
        function CallJav(val) {
            alert(val);
        }
    </script>
</body>
</html>