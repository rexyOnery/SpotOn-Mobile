@page
@model MeetSpotOn.Pages.ConstructionModel
@{
}

<div class="page-content">

    <div data-card-height="cover-card" class="card card-style">
        <div class="card-center text-center">
            <i class="fa fa-clock fa-9x color-highlight mb-5"></i>
            <h1 class="font-36 font-800 mb-2">We're on It!</h1>
            <p class="boxed-text-xl">
                We're currently working to get pack up and running. We estimate we'll be online in about:
            </p>

            <div class="countdown row mb-4 mt-5 ml-3 mr-3">
                <div class="disabled">
                    <h1 class="mb-0 color-theme font-30" id="years"></h1>
                    <p class="mt-n1 color-theme font-11 opacity-30">years</p>
                </div>
                <div class="col-3">
                    <h1 class="mb-0 color-theme font-30" id="days"></h1>
                    <p class="mt-n1 color-theme font-11 opacity-30">days</p>
                </div>
                <div class="col-3">
                    <h1 class="mb-0 color-theme font-30" id="hours"></h1>
                    <p class="mt-n1 color-theme font-11 opacity-30">hours</p>
                </div>
                <div class="col-3">
                    <h1 class="mb-0 color-theme font-30" id="minutes"></h1>
                    <p class="mt-n1 color-theme font-11 opacity-30">minutes</p>
                </div>
                <div class="col-3">
                    <h1 class="mb-0 color-theme font-30" id="seconds"></h1>
                    <p class="mt-n1 color-theme font-11 opacity-30">seconds</p>
                </div>
            </div>

            @*<div class="row mb-0 ml-3 mr-3">
                <div class="col-6">
                    <a href="/" data-back-button class="btn btn-m bg-highlight btn-full rounded-s gradient-highlight font-600 font-13">Back Home</a>
                </div>
                <div class="col-6">
                    <a href="/contact" data-back-button class="btn btn-m bg-highlight btn-full rounded-s gradient-highlight font-600 font-13">Contact</a>
                </div>
            </div>*@
        </div>
    </div>


    @*<div data-menu-load="menu-footer.html"></div>*@
</div>
