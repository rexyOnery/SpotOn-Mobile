@page
@model MeetSpotOn.Pages.IndexModel
@{
    Layout = null;
}


<!DOCTYPE HTML>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover" />
    <title>Spot-On Mobile</title>
    <link rel="stylesheet" type="text/css" href="styles/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="styles/style.css">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="fonts/css/fontawesome-all.min.css">
    <link rel="manifest" href="_manifest.json" data-pwa-version="set_in_manifest_and_pwa_js">
    <link rel="apple-touch-icon" sizes="180x180" href="app/icons/icon-192x192.png">
</head>

<body class="theme-dark">

    <div id="preloader"><div class="spinner-border color-highlight" role="status"></div></div>

    <div id="page">

        <div class="header header-fixed header-logo-left header-auto-show">
            <a href="/" class="header-title">Spot-On</a>
            <a href="#" data-menu="menu-main" class="header-icon header-icon-1"><i class="fas fa-bars"></i></a>
            <a href="#" data-toggle-theme class="header-icon header-icon-3 show-on-theme-dark"><i class="fas fa-sun"></i></a>
            <a href="#" data-toggle-theme class="header-icon header-icon-3 show-on-theme-light"><i class="fas fa-moon"></i></a>
            <a href="/login" class="header-icon header-icon-2"><i class="fas fa-lock"></i></a>
        </div>


        <div class="page-content pb-0">

            <div class="cover-slider owl-carousel">

                <div class="card rounded-0 check-card" data-card-height="cover">
                    <div class="card-top text-center mt-5">
                        <h1 class="font-18 font-700 color-highlight mb-n1">Hello from</h1>
                        <h1 class="font-40 font-800 pb-4">SpotOn</h1>
                        <h4 class="opacity-60 mb-4 pb-4">Explore endless possibilties <br> like never seen before on Mobile.</h4>
                        <h1 class="text-center"><img src="images/undraw/a.svg" class="mx-auto" width="240"></h1>
                    </div>
                </div>
                <div class="card rounded-0 check-card" data-card-height="cover">
                    <div class="card-top text-center  mt-5">
                        <h1 class="font-18 font-700 color-highlight mb-n1">SpotOn is a</h1>
                        <h1 class="font-34 font-800 pb-4">Services Pack</h1>
                        <h4 class="opacity-60 mb-4 pb-4">Add it to your App Collections <br> Use it like your go-to person.</h4>
                        <h1 class="text-center"><img src="images/undraw/b.svg" class="mx-auto" width="180"></h1>
                    </div>
                </div>
                <div class="card rounded-0 check-card" data-card-height="cover">
                    <div class="card-top text-center  mt-5">
                        <h1 class="font-18 font-700 color-highlight mb-n1">Lights off with</h1>
                        <h1 class="font-34 font-800 pb-4">Dark Mode</h1>
                        <h4 class="opacity-60 mb-4 pb-4">Adjusted for your sight <br> Made to suite your Abience.</h4>
                        <h1 class="text-center"><img src="images/undraw/c.svg" class="mx-auto" width="250"></h1>
                    </div>
                </div>
                <div class="card rounded-0 check-card" data-card-height="cover">
                    <div class="card-top text-center  mt-5">
                        <h1 class="font-18 font-700 color-highlight mb-n1">Bootstrapping</h1>
                        <h1 class="font-34 font-800 pb-4">Your Efforts</h1>
                        <h4 class="opacity-60 mb-4 pb-4">Bringing what we can <br> Closer to you.</h4>
                        <h1 class="text-center"><img src="images/undraw/d.svg" class="mx-auto" width="250"></h1>
                    </div>
                </div>
                <div class="card rounded-0 check-card" data-card-height="cover">
                    <div class="card-top text-center  mt-5">
                        <h1 class="font-18 font-700 color-highlight mb-n1">Go Mobile Today</h1>
                        <h1 class="font-34 font-800 pb-4">With SpotOn</h1>
                        <h4 class="opacity-60 mb-4 pb-4">We care about your general well-being.</h4>
                        <h1 class="text-center"><img src="images/undraw/e.svg" class="mx-auto" width="300"></h1>
                    </div>
                </div>

            </div>

            <div class="owl-bottom-button mb-4">
                <a href="/artisan" class="btn btn-m btn-full ml-5 mr-5 rounded-sm border-0 gradient-highlight font-600 font-13 mt-5 scale-box">Get Started</a>
                <a href="/login" class="btn btn-m btn-full ml-5 mr-5 rounded-sm gradient-green color-white font-600 font-13 mt-3">Login</a>
                <a href="/register" class="btn btn-m btn-full ml-5 mr-5 color-red-dark font-600 font-13 mt-3">Register</a>
            </div>


            <!--
                    <div class="card rounded-0 check-card" data-card-height="cover">
                        <div class="card-center text-center pl-3">
                            <h1 class="font-22 font-700 color-highlight mb-n1">Spot-On Is</h1>
                            <h1 class="font-34 font-800 pb-4">PWA Ready</h1>
                            <h4 class="opacity-60 mb-4">Add it to your Home Screen <br> Use it like a Native Application.</h4>
                            <img src="images/undraw/b.svg" class="mt-4 mb-4" width="150">
                            <a href="#" class="btn btn-m btn-full ml-5 mr-5 rounded-sm border-0 gradient-highlight font-600 font-13 mt-5">Get Started</a>
                            <a href="#" class="btn btn-m btn-full ml-5 mr-5 rounded-sm border-highlight color-highlight font-600 font-13 mt-3">Login</a>
                            <p class="mt-4 opacity-40 font-600">Spot-On is a Mobile PWA and Site Template</p>
                        </div>
                    </div>
            -->



        </div>
        <!-- Page content ends here-->
        <!-- Main Menu-->
        <div id="menu-main" class="menu menu-box-left rounded-0" data-menu-load="menu-main.html" data-menu-width="280" data-menu-active="nav-homepages"></div>

        <!-- Share Menu-->
        <div id="menu-share" class="menu menu-box-bottom rounded-m" data-menu-load="menu-share.html" data-menu-height="370"></div>

        <!-- Colors Menu-->
        <div id="menu-colors" class="menu menu-box-bottom rounded-m" data-menu-load="menu-colors.html" data-menu-height="480"></div>


    </div>
    <script type="text/javascript" src="scripts/jquery.js"></script>
    <script type="text/javascript" src="scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="scripts/custom.js"></script>
    <script type="text/javascript">
        //localStorage.clear();
        function CallJav(val) {
            alert(val);
        }
    </script>
</body>
</html>